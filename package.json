    "immutable": "^4.0.0-rc.12",
    "jsonapi-deserializer": "^0.1.1",
    "ng2-ui-auth": "^9.2.0",
    "ngx-bootstrap": "^3.1.1",
    "ngx-toastr": "^9.1.1",
    "reselect": "^4.0.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "ts-loader": "^5.3.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "zone.js": "^0.8.26"
