    "jsonapi-deserializer": "^0.1.1",
    "ng2-ui-auth": "^9.0.0",
    "ngx-bootstrap": "^3.0.1",
    "ngx-toastr": "^8.10.0",
    "reselect": "^3.0.1",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "ts-loader": "^4.4.2",
    "web-animations-js": "^2.3.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.7.2",
    "@angular/cli": "^6.1.2",
